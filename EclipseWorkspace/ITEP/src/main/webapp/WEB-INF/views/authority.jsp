                    <tr>
                        <td>${row.emn}</td>
                        <td>${row.emm}</td>
                        <td>${row.krn_brm}</td>
                        <td>${row.auth_cd}</td>
                        <td>${row.auth_nm}</td>
                        <td>${row.reg_dt}</td>
                        
                    </tr>
                </c:forEach>
            </c:when>
            <c:otherwise>
                <tr>
                    <td colspan="4">조회된 결과가 없습니다.</td>
                </tr>
            </c:otherwise>
        </c:choose>
         
