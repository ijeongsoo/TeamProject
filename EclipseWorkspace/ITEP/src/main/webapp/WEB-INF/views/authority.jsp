        <col width="5%"/>
        <col width="5%"/>
        <col width="10%"/>
        <col width="10%"/>

    </colgroup>
    <thead>
        <tr>
            <th scope="col">사번</th>
            <th scope="col">이름</th>
            <th scope="col">부서명</th>
            <th scope="col">권한코드</th>
            <th scope="col">권한명</th>
            <th scope="col">등록일자</th>            
        </tr>
    </thead>
    <tbody>
        <c:choose>
            <c:when test="${fn:length(List) > 0}">
