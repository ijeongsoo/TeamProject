package com.IBK.study02;

public class Book {
	private String isbn;
	private String title;
	private String autho;
	private String publisher;
	private int price;
	private String desc="";
	
	public Book() {
		// TODO Auto-generated constructor stub
	}

	public Book(String isbn, String title, String autho, String publisher, int price, String desc) {
		super();
		this.isbn = isbn;
		this.title = title;
		this.autho = autho;
		this.publisher = publisher;
		this.price = price;
		this.desc = desc;
	}
	
	public Book(String isbn, String title, String autho){
		this.isbn = isbn;
		this.title = title;
		this.autho = autho;
	}
	
	public String toString(){
		String msg = isbn +"\t|"+title+"\t|"+autho+"\t|"+publisher+"\t|"+price+"\t|"+desc;
		return msg;
	}
}
