package com.IBK.study03;

public class VipCustomer extends Custormer{
	public VipCustomer() {
		// TODO Auto-generated constructor stub
	}
	
	public VipCustomer(String name, int age, String hobby){
		super(name, age, hobby);
		
	}

	@Override
	public String toString() {
		return "VipCustomer [name=" + name + ", age=" + age + ", hobby=" + hobby + ", getName()=" + getName()
				+ ", getAge()=" + getAge() + ", getHobby()=" + getHobby() + ", toString()=" + super.toString()
				+ ", getClass()=" + getClass() + ", hashCode()=" + hashCode() + "]";
	}
	
	
	
}
