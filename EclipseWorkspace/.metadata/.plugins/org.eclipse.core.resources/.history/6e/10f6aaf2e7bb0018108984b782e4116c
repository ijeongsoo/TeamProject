package com.IBK.study03.project2;

import com.IBK.study02.project.Book;

public class MainTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Book book = new Book("1", "이정수책", "이정수", "이정수출판사", 100, "이정수 책입니다.");
		Book book2 = new Book("2", "이정수책2", "이정수", "이정수출판사", 100, "이정수 책입니다.");

		IManager man = new ArrayManager();
		
		man.add(book);
		man.add(book2);
		Book[] bookArr= man.selectToAll();
		for(int i=0; i<bookArr.length; i++){
			if(bookArr[i] == null){
				break;
			}else{
				System.out.println(bookArr[i].toString());
				
			}
		}
	
	}

}
