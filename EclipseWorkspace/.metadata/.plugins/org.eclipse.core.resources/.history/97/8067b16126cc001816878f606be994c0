<mapper namespace="emp001m">

	<select id= "selectByEmn" resultType="kr.co.ibk.itep.dto.EmpJoinedDep" parameterType="string">
		select emn, emm, blng_brcd, auth_cd, brcd, krn_brm from TB_IEP_EMP001M e, TB_IEP_BRI001M b where e.brcd = b.brcd and emn=#{emn}	

	</select>
	

	
