package com.IBK.study03.project2;

import com.IBK.study02.project.Book;

public class ArrayManager implements IManager {
	private Book[] books;
	public ArrayManager() {
		// TODO Auto-generated constructor stub
		books = new Book[10];
	}
	
	@Override
	public void add(Book b) {
		// TODO Auto-generated method stub
		boolean flag=false;
		for(int i=0; i<10; i++){
			if(books[i]==null){
				books[i]= b;
				flag=true;
				break;
			}
		}
		if(flag=false){
			System.out.println("배열이 이미가득 찼습니다.");
		}
		
	}

	@Override
	public int update(Book b) {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public void delete(int isbn) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public Book select(String title) {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public Book[] selectToAll() {
		// TODO Auto-generated method stub
		return null;
	}

}
